<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="defaultItemValueInterceptor"
          class="toker.panel.aop.interceptor.DefaultItemValueInterceptor"
          factory-method="aspectOf">
        <property name="itemRepo" ref="itemRepository" />
    </bean>

    <bean id="logFetchingLogging"
          class="toker.panel.aop.logging.LogFetchingLogging"
          factory-method="aspectOf">
        <property name="userRepo" ref="panelUserRepository" />
        <property name="logRepo" ref="logRepository" />
    </bean>

    <bean id="sessionEnder"
          class="toker.panel.aop.SessionEnder"
          factory-method="aspectOf">
        <property name="panelUserRepository" ref="panelUserRepository" />
        <property name="sessionRepository" ref="panelUserSessionRepository" />
        <property name="assignmentRepository" ref="panelUserAuthorityAssignmentRepository" />
    </bean>
</beans>