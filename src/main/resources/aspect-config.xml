<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <bean class="toker.warbandscripts.panel.aspect.LogsAspect" factory-method="aspectOf">
        <property name="logger" ref="logger"></property>
    </bean>

    <bean class="toker.warbandscripts.panel.aspect.PlayerLoggingAspect" factory-method="aspectOf">
        <property name="logger" ref="logger"></property>
    </bean>

    <bean class="toker.warbandscripts.panel.aspect.BanLoggingAspect" factory-method="aspectOf">
        <property name="logger" ref="logger"></property>
    </bean>

    <bean class="toker.warbandscripts.panel.aspect.DoorLoggingAspect" factory-method="aspectOf">
        <property name="logger" ref="logger"></property>
    </bean>

    <bean class="toker.warbandscripts.panel.aspect.PanelUserLoggingAspect" factory-method="aspectOf">
        <property name="logger" ref="logger"></property>
    </bean>
</beans>